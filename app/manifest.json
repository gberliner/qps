{
  "name": "Quick Page Saver",
  "short_name": "Page Saver",
  "description": "Save webpages to your Google Drive",
  "version": "0.0.0",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "options_page": "pages/options.html",
  "options_ui": {
    "page": "pages/options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "styles/contentscript.css"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "browserSettings",
    "contextMenus",
    "contextualIdentities",
    "identity",
    "menus",
    "notifications",
    "printerProvider",
    "webRequest",
    "webRequestBlocking"
  ],
  "oauth2": {
    "client_id": "908093233941-0j2cam42nd0972vg73i5n9li9c1pq5bj.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file"
    ]
  }
}
